{
    "id": "org.kde.kdebugsettings",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.6-kf6preview",
    "sdk": "org.kde.Sdk",
    "command": "kdebugsettings",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": ["--share=ipc", "--socket=fallback-x11", "--socket=wayland" ],

    "modules": [
        {
            "name": "icon",
            "buildsystem": "simple",
            "build-commands": [
                "mkdir -p /app/share/icons/breeze/actions/24/",
                "install -D /usr/share/icons/breeze/actions/24/debug-run.svg /app/share/icons/breeze/actions/24/"
            ]
        },
        {
            "name": "kdebugsettings",
            "buildsystem": "cmake-ninja",
            "sources": [ { "type": "dir", "path": "." } ]
        }
    ]
}
